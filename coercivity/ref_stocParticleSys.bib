@incollection{albeverio2003_StrongFeller,
  address = {{Providence, Rhode Island}},
  title = {Strong {{Feller}} Properties for Distorted {{Brownian}} Motion and Applications to Finite Particle Systems with Singular Interactions},
  volume = {317},
  isbn = {978-0-8218-3202-8 978-0-8218-7907-8},
  abstract = {We prove strong Feller properties for a class of distorted Brownian motions on d. We also construct a weak solution to the corresponding stochastic differential equation starting from any point in \{ = 0\} and staying in \{ = 0\} before possibly going out of any ball in d. Here is the Lebesgue density of the symmetrizing measure {$\mathrm{\mu}$}. Our condition on the logarithmic derivative {$\nabla$} is that it should be locally in Ld+{$\epsilon$}, but only with respect to the symmetrizing measure {$\mathrm{\mu}$} = dx, not necessarily Lebesgue measure dx. This allows applications to singular situations. In particular, finite particle systems with two body interactions with infinitely strong repulsion can be treated by our results. Among other things it is shown that they never meet no matter what their starting configuration was. Another application treats diffusions in random media.},
  booktitle = {Contemporary {{Mathematics}}},
  publisher = {{American Mathematical Society}},
  doi = {10.1090/conm/317/05517},
  author = {Albeverio, Sergio and Kondratiev, Yuri and R{\"o}ckner, Michael},
  editor = {Kuo, Hui-Hsiung and Sengupta, Ambar N.},
  year = {2003},
  pages = {15-35}
}


