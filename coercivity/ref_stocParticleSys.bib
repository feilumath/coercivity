@article{skorokhod1996_RegularityManyparticle,
  title = {On the Regularity of Many-Particle Dynamical Systems Perturbed by White Noise},
  volume = {9},
  issn = {1048-9533, 1687-2177},
  abstract = {We consider a system of finite number of particles that are moving in              
              under mutual interaction. It is assumed that the particles are subjected to some 
additional random forces which cause diffusion motion of the particles. The latter is described by a system of stochastic differential equations of the first order 
for noninertia particles and the second order for inertial particles. The coefficient of the system are unbounded because the interaction force tends to infinity 
if the distance between two particles tends to zero. The system is called regular 
if no particle can hit the other. We investigate conditions of regularity.  This article is dedicated to the memory of Roland L. Dobrushin.},
  number = {4},
  journal = {Journal of Applied Mathematics and Stochastic Analysis},
  doi = {10.1155/S1048953396000378},
  author = {Skorokhod, Anatoli V.},
  year = {1996},
  pages = {427-437}
}



